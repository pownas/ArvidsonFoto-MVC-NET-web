<!DOCTYPE html>
<html lang="sv-se">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="ArvidsonFoto-version" content="v0.7.1 - https://github.com/pownas/ArvidsonFoto-MVC-NET5/tree/9160b2b " /> <!-- Bör vara: /tree/ - för visning av filträdet... (Annars fungerar också: /commit/ ) -->
    @await RenderSectionAsync("MetaData", required: false)
    <title>@ViewData["Title"] - ArvidsonFoto.se</title>
    <base href="~/" />
    @*<!-- Se info i "site.scss" för dessa imports... -->*@
    @*<link rel="stylesheet" href="lib/bootstrap/css/bootstrap.min.css" />*@
    @*<link rel="stylesheet" href="lib/jQuery.smartmenus/css/sm-core-css.css" />*@
    @*<link rel="stylesheet" href="lib/jQuery.smartmenus/css/sm-mint/sm-mint.css" />*@
    @*<link rel="stylesheet" href="lib/jQuery.smartmenus/css/sm-mint-addon.css" />*@
    @*<link rel="stylesheet" href="lib/bootstrap-icons/bootstrap-icons.min.css" />*@
    @*<link rel="stylesheet" href="lib/glightbox/css/glightbox.min.css" />*@
    <link rel="stylesheet" href="css/site.min.css" />
</head>
<body>
    <header>
        <partial name="_NavBar" />
    </header>
    <main role="main" class="container-md pb-3">
        @RenderBody()
    </main>
    <footer class="footer overridden-footer border-top">
        <div class="container">
            <partial name="_Footer" />
        </div>
    </footer>
    <script src="lib/jquery/dist/jquery.min.js"></script> @*<!-- version: 3.5.1 -->*@
    <script src="lib/bootstrap/js/bootstrap.min.js"></script> @*<!-- version: 4.5.3-dist -->*@
    <script src="lib/jQuery.smartmenus/jquery.smartmenus.min.js"></script> @*<!-- version: 1.1.1 -->*@
    <script src="lib/jQuery.smartmenus/smartmenu.js"></script> @*<!-- version: 1.1.1 -->*@
    <script src="lib/glightbox/js/glightbox.min.js"></script> @*<!-- version: 3.0.7 -->*@
    <script src="js/gLightBoxOptions.js"></script> @*<!-- Egna inställningar till glightbox -->*@
    <script src="js/site.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>